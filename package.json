{
  "name": "onpass",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/onpass/onpass-client"
  },
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "webpack --mode production",
    "test": "jest",
    "doc": "node_modules/.bin/jsdoc -c jsdoc.json"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "babel-register": "^6.26.0",
    "jsdoc-vue": "^1.0.0",
    "style-loader": "^1.2.1",
    "vee-validate": "^2.2.8",
    "vue": "^2.6.10",
    "vue-flip": "^1.0.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "^6.26.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-vue": "^2.0.2",
    "better-docs": "^2.0.1",
    "css-loader": "^3.5.3",
    "deepmerge": "^4.2.2",
    "fibers": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.0.1",
    "jest-serializer-vue": "^2.0.2",
    "jsdoc": "^3.6.4",
    "path": "^0.12.7",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^3.0.5",
    "vue-loader": "^14.2.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
